package documin;

import static org.junit.Assert.assertEquals;

class TestTermo {
	Termo termo = new Termo(1, "Teste / termos / Aleat√≥rios" , null, "/", "");

	void testTermoGerarRepresentacaoCompleta() {
		assertEquals(termo.gerarRepresentacaoCompleta(), "");
	}
	
	void testTermoGerarRepresentacaoResumida() {
		assertEquals(termo.gerarRepresentacaoResumida(), "");
	}
}
